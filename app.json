{
  "expo": {
    "name": "EthioCarsApp",
    "slug": "EthioCarsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ethiocarsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/logo-ethiocars.jpg",
      "backgroundColor": "#f8f6f6",
      "resizeMode": "contain"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-ethiocars.jpg",
          "backgroundColor": "#f8f6f6",
          "resizeMode": "contain"
        }
      ]
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ethiocars.app"
    },
    "android": {
      "package": "com.ethiocars.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    }
  }
}
